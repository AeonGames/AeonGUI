{
    "git.path": "${GIT_EXECUTABLE}",
    "git.ignoreLimitWarning": true,
    "terminal.integrated.shell.windows": "${BASH_EXECUTABLE}",
    "terminal.integrated.shellArgs.windows": ["--login", "-i"],
    "terminal.integrated.env.windows":
