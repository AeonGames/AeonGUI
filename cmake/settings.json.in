{
    "git.path": "${GIT_EXECUTABLE}",
    "git.ignoreLimitWarning": true,
    "terminal.integrated.profiles.windows": {
        "MinGW GCC Bash": {
            "path": "${BASH_EXECUTABLE}",
            "args": ["--login", "-i"],
            "env":
            {
                "MSYSTEM": "MINGW64",
                "CHERE_INVOKING":"1"
            }
        },
        "MinGW GCC Zsh": {
            "path": "${ZSH_EXECUTABLE}",
            "args": ["--login", "-i"],
            "env":
            {
                "MSYSTEM": "MINGW64",
                "CHERE_INVOKING":"1"
            }
        },        
        "MinGW Clang Bash": {
            "path": "${ZSH_EXECUTABLE}",
            "args": ["--login", "-i"],
            "env":
            {
                "MSYSTEM": "CLANG64",
                "CHERE_INVOKING":"1"
            }
        },
        "MinGW Clang Zsh": {
            "path": "${ZSH_EXECUTABLE}",
            "args": ["--login", "-i"],
            "env":
            {
                "MSYSTEM": "CLANG64",
                "CHERE_INVOKING":"1"
            }
        },
        "MinGW UCRT64 Bash": {
            "path": "${BASH_EXECUTABLE}",
            "args": ["--login", "-i"],
            "env":
            {
                "MSYSTEM": "UCRT64",
                "CHERE_INVOKING":"1"
            }
        },
        "MinGW UCRT64 Zsh": {
            "path": "${ZSH_EXECUTABLE}",
            "args": ["--login", "-i"],
            "env":
            {
                "MSYSTEM": "UCRT64",
                "CHERE_INVOKING":"1"
            }
        },
        "PowerShell":null,
        "Command Prompt":null
    },  
    "python.defaultInterpreterPath": "${Python3_EXECUTABLE}",
    "clang.executable": "${CLANG_EXECUTABLE}",
    "cmake.cmakePath": "${CMAKE_COMMAND}",
    "window.zoomLevel": ${CODE_ZOOMLEVEL},
    "files.associations": {
        "*.json.in": "json",
        "*.cap.in": "json",
        "*.cap": "json",
        "*.cmake": "cmake",
        "*.cmake.in": "cmake",
        "*.ypp": "yacc",
    },
    "editor.fontLigatures": true,
    "editor.fontFamily": "'FiraCode Nerd Font Mono', 'FiraCode Nerd Font', 'FiraCode NF', 'Consolas', 'Courier New', monospace",
    "terminal.integrated.fontFamily": "'MesloLGS Nerd Font Mono', 'Consolas', monospace"
}
