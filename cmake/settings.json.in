        "MSYSTEM": "MINGW64",
        "CHERE_INVOKING": "1"
    },
    "cmake.cmakePath": "${CMAKE_COMMAND}",
    "window.zoomLevel": ${CODE_ZOOMLEVEL},
    "files.associations": {
        "*.json.in": "json",
        "*.ypp": "yacc"
    }
}
